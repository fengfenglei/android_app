<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <include layout="@layout/common_title_bar" />
    <LinearLayout
        android:id="@+id/all_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone"
        >
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/home_item_left_right_space_30"
    android:orientation="horizontal">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/home_item_left_right_space_30"
        android:text="订单编号："
        android:textColor="@color/main_text_tow_color"
        android:textSize="@dimen/font_size_14"/>
    <TextView
        android:id="@+id/number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/main_text_tow_color"
        android:textSize="@dimen/font_size_14"
        android:text="0.00"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
      />
    <TextView
        android:id="@+id/order_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/blue"
        android:textSize="@dimen/font_size_14"
        android:text="详情"/>
</LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginRight="@dimen/home_item_left_right_space_15"
        android:layout_marginLeft="@dimen/home_item_left_right_space_15"
        android:background="@color/line_color"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/home_item_left_right_space_30"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/home_item_left_right_space_30"
            android:text="订单状态："
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"/>
        <TextView
            android:id="@+id/state"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"
            android:text="交易类型"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/home_item_left_right_space_30"
        android:orientation="horizontal">
        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/home_item_left_right_space_30"
            android:text="下单时间："
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"/>
        <TextView
            android:id="@+id/time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"
            android:text="交易日期"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/home_item_left_right_space_30"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/home_item_left_right_space_30"
            android:text="确认收货："
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"/>
        <TextView
            android:id="@+id/receiving_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"
            android:text="流水号"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/home_item_left_right_space_30"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/home_item_left_right_space_30"
            android:text="预计激活时间："
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"/>
        <TextView
            android:id="@+id/activation_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"
            android:text="交易说明"/>

    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/home_item_left_right_space_30"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/home_item_left_right_space_30"
                android:text="订单总额："
                android:textColor="@color/main_text_tow_color"
                android:textSize="@dimen/font_size_14"/>
            <TextView
                android:id="@+id/all_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/main_text_tow_color"
                android:textSize="@dimen/font_size_14"
                android:text="交易说明"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/home_item_left_right_space_30"
            android:orientation="horizontal">
            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/home_item_left_right_space_30"
                android:text="预计激活："
                android:textColor="@color/main_text_tow_color"
                android:textSize="@dimen/font_size_14"/>
            <TextView
                android:id="@+id/activation_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/main_text_tow_color"
                android:textSize="@dimen/font_size_14"
                android:text="交易说明"/>

        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="@color/line_color"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/home_item_left_right_space_30"
            android:text="商品明细："
            android:textColor="@color/main_text_tow_color"
            android:textSize="@dimen/font_size_14"/>
        <!--商品-->
        <LinearLayout
            android:id="@+id/shop_details"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/home_item_left_right_space_15"
            android:orientation="horizontal">
            <!--商品-->
            <ImageView
                android:id="@+id/write_order_shop_icon"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/icon_bg"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/home_item_left_right_space_15"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/shop_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我是商品名称"
                    android:maxLines="2"
                    android:enabled="true"
                    android:textColor="@color/main_text_tow_color"
                    android:textSize="@dimen/font_size_14"/>
                <TextView
                    android:id="@+id/guige"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我是商品名称"
                    android:maxLines="1"
                    android:enabled="true"
                    android:layout_marginTop="5dp"
                    android:textColor="@color/main_text_three_color"
                    android:textSize="@dimen/font_size_12"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="50.00"
                        android:maxLines="2"
                        android:enabled="true"
                        android:textColor="@color/text_color_red"
                        android:textSize="@dimen/font_size_14"/>


                    <TextView
                        android:id="@+id/shop_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="x1"
                        android:layout_marginLeft="@dimen/home_item_left_right_space_30"
                        android:maxLines="1"
                        android:enabled="true"
                        android:layout_marginRight="@dimen/home_item_left_right_space_15"
                        android:textColor="@color/main_text_tow_color"
                        android:textSize="@dimen/font_size_14"
                        />

                </LinearLayout>
            </LinearLayout>
</LinearLayout>
</LinearLayout></LinearLayout>